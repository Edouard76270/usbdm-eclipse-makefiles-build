<?xml version="1.0"?>
<wizardExtension>
   <newProjectExtension>
      <launchSettings>
         <settingsGroup id="com.processorexpert.launch.gdb.usbdm">
            <applySetting type="string"  id="net.sourceforge.usbdm.gdb.bdmSerialNumber"                   value="[Any connected USBDM]" />
            <applySetting type="string"  id="net.sourceforge.usbdm.gdb.autoReconnect"                     value="AUTOCONNECT_ALWAYS" />
