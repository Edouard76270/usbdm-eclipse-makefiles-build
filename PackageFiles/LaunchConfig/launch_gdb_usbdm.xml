            <applySetting type="integer"  id="org.eclipse.cdt.debug.gdbjtag.core.delay"                   value="0"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.doHalt"                  value="false"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.doReset"                 value="false"/>
