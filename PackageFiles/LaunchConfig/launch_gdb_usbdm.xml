            <applySetting type="integer" id="net.sourceforge.usbdm.gdb.connectionTimeout"                 value="10" />

            <applySetting type="string"  id="net.sourceforge.usbdm.gdb.deviceName"                        value="M${DebuggerPnEId}" />
