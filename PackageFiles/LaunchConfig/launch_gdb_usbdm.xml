            <applySetting type="string"  id="net.sourceforge.usbdm.gdb.gdbCommand"                        value="${usbdm_armLtd_arm_prefix}gdb" />

            <applySetting type="boolean"  id="net.sourceforge.usbdm.gdb.restartUsesStartup"               value="true"/>
            <applySetting type="string"   id="net.sourceforge.usbdm.gdb.securityOption"                   value="SECURITY_SMART"/>
            <applySetting type="string"   id="net.sourceforge.usbdm.gdb.serverType"                       value="SERVER_SOCKET"/>
            <applySetting type="string"   id="net.sourceforge.usbdm.gdb.targetVdd"                        value="BDM_TARGET_VDD_3V3"/>
            <applySetting type="boolean"  id="net.sourceforge.usbdm.gdb.trimClock"                        value="false"/>
