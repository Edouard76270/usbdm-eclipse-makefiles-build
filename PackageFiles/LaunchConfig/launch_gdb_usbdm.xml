      </launchSettings>
   </newProjectExtension>
   <uiExtension>
      <conditions>
         <condition
            id="com.processorexpert.condition.launch.gnuarm.gdb.usbdm">
            <or>
               <conditionRef conditionId="com.processorexpert.condition.false" />
            </or>
         </condition>
      </conditions>
   </uiExtension>

</wizardExtension>
