            <applySetting type="string"   id="org.eclipse.cdt.debug.gdbjtag.core.runCommands"             value=""/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.setPcRegister"           value="false"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.setResume"               value="true"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.setStopAt"               value="true"/>
            <applySetting type="string"   id="org.eclipse.cdt.debug.gdbjtag.core.stopAt"                  value="main"/>
            <applySetting type="string"   id="org.eclipse.cdt.debug.gdbjtag.core.symbolsFileName"         value=""/>
            <applySetting type="string"   id="org.eclipse.cdt.debug.gdbjtag.core.symbolsOffset"           value=""/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.useFileForImage"         value="false"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.useFileForSymbols"       value="false"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.useProjBinaryForImage"   value="true"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.useProjBinaryForSymbols" value="true"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.gdbjtag.core.useRemoteTarget"         value="true"/>
            <applySetting type="string"   id="org.eclipse.cdt.debug.mi.core.DEBUG_NAME"                   value="${usbdm_armLtd_arm_path}/${usbdm_armLtd_arm_prefix}gdb"/>
            <applySetting type="string"   id="org.eclipse.cdt.debug.mi.core.commandFactory"               value="Standard"/>
            <applySetting type="string"   id="org.eclipse.cdt.debug.mi.core.protocol"                     value="mi"/>
            <applySetting type="boolean"  id="org.eclipse.cdt.debug.mi.core.verboseMode"                  value="false"/>
            <applySetting type="string"   id="org.eclipse.cdt.dsf.gdb.DEBUG_NAME"                         value="${usbdm_armLtd_arm_path}/${usbdm_armLtd_arm_prefix}gdb"/>
            <applySetting type="integer"  id="org.eclipse.cdt.launch.ATTR_BUILD_BEFORE_LAUNCH_ATTR"       value="2"/>
            <applySetting type="string"   id="org.eclipse.cdt.launch.COREFILE_PATH"                       value=""/>
            <applySetting type="boolean"  id="org.eclipse.cdt.launch.PROJECT_BUILD_CONFIG_AUTO_ATTR"      value="true"/>
            <applySetting type="string"   id="org.eclipse.cdt.launch.PROJECT_BUILD_CONFIG_ID_ATTR"        value=""/>
            <applySetting type="boolean"  id="org.eclipse.cdt.launch.use_terminal"                        value="true"/>
