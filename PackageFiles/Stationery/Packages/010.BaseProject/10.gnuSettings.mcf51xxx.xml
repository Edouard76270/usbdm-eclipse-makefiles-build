         <or>
            <deviceFamilyIs>CFV1</deviceFamilyIs>
            <deviceFamilyIs>CFV1Plus</deviceFamilyIs>
         </or>
      </applyWhen>
      <projectOption id="net.sourceforge.usbdm.cdt.coldfire.toolchain.mcpu">
         <value>net.sourceforge.usbdm.cdt.coldfire.toolchain.mcpu.51</value>
      </projectOption>
      <projectOption id="net.sourceforge.usbdm.cdt.arm.toolchain.buildtools">
         <value>net.sourceforge.usbdm.cdt.toolchain.processor.usbdmConfigure.codesourceryColdfire</value>
      </projectOption>
   </projectActionList>
</root>