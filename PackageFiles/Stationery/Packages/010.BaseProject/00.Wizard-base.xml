<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0" >

   <projectActionList id="Wizard-base" >
      <page id="usbdm-project-options-page" name="USBDM Project Options" description="Select Project Options">
         <group id="group-additional-options"
            name="Additional Project Options ($(targetDevice))"
            row="0"
            width="1"
          />
         <group id="group-ksdk-operating-system"
            name="Kinetis SDK Operating System Support"
            row="1"
            width="1"
          />
      </page>
      <page id="kinetis-additional-options-page" name="Additional Options" description="Select Additional Options">
         <group id="group-peripheral-options"
            name="Internal Peripherals"
            row="1"
            width="1"
            span="1"
          />
         <group id="group-i2c-peripheral-options"
            name="I2C Devices"
            row="1"
            width="1"
            span="1"
          />
         <group id="group-serial-options"
            name="Serial Peripherals"
            row="1"
            width="1"
            span="1"
          />
         <group id="group-spi-peripheral-options"
            name="SPI Devices"
            row="1"
            width="1"
            span="1"
          />
         <group id="group-ksdk-demo"
            name="Kinetis SDK Demo"
            row="2"
            width="2"
            span="1"
          />
         <group id="group-ksdk-usb-demo"
            name="Kinetis SDK Demo"
            row="2"
            width="2"
            span="1"
          />
      </page>
      
      <!-- Variables defined externally by New project Wizard -->
      <!-- Default power option -->
      <constant id="bdmTargetVdd" value="BDM_TARGET_VDD_OFF" weak="true" />
   </projectActionList>
</root>
