<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0" >
   <!-- Freescale group-ksdk-demos -->
   <projectActionList id="demo.KSDK-usb-host-hid-keyboard-mouse" root="">
      <applyWhen>
         <and>
            <variableRef idRef="demo.KSDK-usb-host-hid-keyboard-mouse" />
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <variable id="projectOptionValue.KSDK-usb-host-hid-keyboard-mouse" 
            name="USB Host HID keyboard-mouse example" 
            description="The board will transfer data from an attached USB keyboard and mouse"
            radioGroup="group-ksdk-demo" >
         <enableWhen>
            <and>
               <requirement idRef="projectOptionValue.Kinetis-SDK-enable" />
               <or>
                  <requirement idRef="projectOptionValue.Kinetis-SDK-os-bare-metal" />
                  <and>
                     <variableRef idRef="demo.KSDK-usb-with-rtos"/>
                     <or>
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-free-rtos" />
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-mqx" />
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-ucosii" />
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-ucosiii" />
                     </or>
                  </and>
               </or>
            </and>
         </enableWhen>
      </variable>
      <projectActionList>
         <applyWhen>
            <and>
               <variableRef idRef="projectOptionValue.Kinetis-SDK-enable" />
               <variableRef idRef="projectOptionValue.KSDK-usb-host-hid-keyboard-mouse" />
            </and>
         </applyWhen>
         <deleteResource target="Sources/main.c" /> 
         
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/readme.pdf" type="link" target="doc/readme.pdf"      sourcePathType="absolute" />
         
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/hardware_init.c"    target="board/hardware_init.c"   sourcePathType="absolute" />
         
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/hidkeyboard.c"    target="Sources/hidkeyboard.c"          sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/hidkeyboard.h"    target="Sources/hidkeyboard.h"          sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/hidmouse.c"       target="Sources/hidmouse.c"             sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/hidmouse.h"       target="Sources/hidmouse.h"             sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/mouse_keyboard.c" target="Sources/mouse_keyboard.c"       sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/host/hid/keyboard+mouse/mouse_keyboard.h" target="Sources/mouse_keyboard.h"       sourcePathType="absolute" />
         <copy source="usbdm_kds_path/platform/system/src/hwtimer/fsl_hwtimer_pit_irq.c"                                     target="Sources/fsl_hwtimer_pit_irq.c"  sourcePathType="absolute" />

         <!-- C Include paths -->
         <projectOption id="net.sourceforge.usbdm.gnu.c.compiler.option.include.paths">
            <value>&quot;${usbdm_kds_path}/usb/facility/include&quot;</value>
