<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE root SYSTEM "../package.dtd">

<root version="1.0" >
   <!-- Freescale group-ksdk-demos -->
   <projectActionList id="KSDK-usb-cdc-virtual-com" root="">
      <applyWhen>
         <and>
            <variableRef idRef="demo.KSDK-usb-cdc-virtual-com" />
            <variableRef idRef="outputType" condition="matches" value=".*ProjectType\.exe$" />
         </and>
      </applyWhen>
      <variable id="projectOptionValue.KSDK-usb-cdc-virtual-com" 
            name="USB CDC Virtual Comm Example" 
            description="The board will enumerate as a USB CDC serial device"
            radioGroup="group-ksdk-demo" >
         <enableWhen>
            <and>
               <requirement idRef="projectOptionValue.Kinetis-SDK-enable" />
               <or>
                  <requirement idRef="projectOptionValue.Kinetis-SDK-os-bare-metal" />
                  <and>
                     <variableRef idRef="demo.KSDK-usb-with-rtos"/>
                     <or>
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-free-rtos" />
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-mqx" />
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-ucosii" />
                        <requirement idRef="projectOptionValue.Kinetis-SDK-os-ucosiii" />
                     </or>
                  </and>
               </or>
            </and>
         </enableWhen>
      </variable>
      <projectActionList>
         <applyWhen>
            <and>
               <variableRef idRef="projectOptionValue.Kinetis-SDK-enable" />
               <variableRef idRef="projectOptionValue.KSDK-usb-cdc-virtual-com" />
            </and>
         </applyWhen>
         <deleteResource target="Sources/main.c" /> 
         
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/device/cdc/virtual_com/readme.pdf"        type="link" target="doc/readme.pdf"             sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/device/cdc/virtual_com/inf/fsl_ucwxp.inf" type="link" target="doc/fsl_ucwxp.inf"          sourcePathType="absolute" />

         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/device/cdc/virtual_com/hardware_init.c"    target="board/hardware_init.c"      sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/device/cdc/virtual_com/usb_descriptor.c"   target="Sources/usb_descriptor.c"   sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/device/cdc/virtual_com/usb_descriptor.h"   target="Sources/usb_descriptor.h"   sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/device/cdc/virtual_com/virtual_com.c"      target="Sources/virtual_com.c"      sourcePathType="absolute" />
         <copy source="usbdm_kds_path/examples/$(demo.KDS.boardName)/demo_apps/usb/device/cdc/virtual_com/virtual_com.h"      target="Sources/virtual_com.h"      sourcePathType="absolute" />

         <!-- C Include paths -->
         <projectOption id="net.sourceforge.usbdm.gnu.c.compiler.option.include.paths">
            <value>&quot;${usbdm_kds_path}/usb/adapter/sources&quot;</value>
            <value>&quot;${usbdm_kds_path}/usb/adapter/sources/sdk&quot;</value>
            <value>&quot;${usbdm_kds_path}/usb/usb_core/device/include/$(demo.KDS.boardName)&quot;</value>
            <value>&quot;${usbdm_kds_path}/usb/usb_core/device/include&quot;</value>
            <value>&quot;${usbdm_kds_path}/usb/usb_core/device/sources/classes/audio&quot;</value>
            <value>&quot;${usbdm_kds_path}/usb/usb_core/device/sources/classes/cdc&quot;</value>
            <value>&quot;${usbdm_kds_path}/usb/usb_core/device/sources/classes/common&quot;</value>
